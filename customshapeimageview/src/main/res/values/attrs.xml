<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomShapeImageView">
        <attr name="shape" format="enum">
            <enum name="circle" value="0"/>
            <enum name="rhombus" value="1"/>
            <enum name="rounrectangle" value="2"/>
            <enum name="oval" value="3"/>
            <enum name="hexagon" value="4"/>
        </attr>
        <attr name="corner" format="dimension"/>
        <attr name="rotate" format="integer"/>
    </declare-styleable>
</resources>